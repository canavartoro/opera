--SELECT * FROM TBLDEPMAS WITH (NOLOCK)

CREATE VIEW [Barset].[V_Depolar] AS
SELECT Barset.fn_MikrobarKey(DEPO_KODU) AS DepoId, DEPO_KODU AS DepoKod, Barset.fn_Turkcelestir(DEPO_ISMI) AS DepoAd, DEPO_KODU AS Barkod, CASE WHEN ISNULL(DEPO_KILITLE, 'H') = N'E' THEN 1 ELSE 0 END AS Kalite, ISNULL(CARI_KODU, '') AS Aciklama, CASE WHEN EKSIBAKIYE = N'E' THEN 1 ELSE 0 END AS EksiStok FROM dbo.TBLSTOKDP WITH (NOLOCK) WHERE SUBE_KODU IN (2, 6)
